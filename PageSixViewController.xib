                        <label opaque="NO" clipsSubviews="YES" userInteractionEnabled="NO" contentMode="left" text="Carrie Steele Logan" lineBreakMode="tailTruncation" minimumFontSize="10" id="11">
                            <rect key="frame" x="60" y="975" width="280" height="21"/>
